l_japanese:

 # GetFullName
 custom_loc_universal_full_name_default:0 "[ROOT.GetCharacter.GetFirstName]$custom_loc_universal_dot_formatted$[ROOT.GetCharacter.GetLastName]"
 custom_loc_universal_full_name_reverse:0 "[ROOT.GetCharacter.GetLastName]$custom_loc_universal_dot_formatted$[ROOT.GetCharacter.GetFirstName]"
 custom_loc_universal_full_name_reverse_chinese:0 "$custom_loc_universal_full_name_reverse$"
 custom_loc_universal_full_name_reverse_japanese:0 "[ROOT.GetCharacter.GetLastName][ROOT.GetCharacter.GetFirstName]"

 # GetFullNameNoFormatting
 custom_loc_universal_full_name_no_formatting_default:0 "[ROOT.GetCharacter.GetFirstNameNoFormatting]$custom_loc_universal_dot$[ROOT.GetCharacter.GetLastNameNoFormatting]"
 custom_loc_universal_full_name_no_formatting_reverse:0 "[ROOT.GetCharacter.GetLastNameNoFormatting]$custom_loc_universal_dot$[ROOT.GetCharacter.GetFirstNameNoFormatting]"
 custom_loc_universal_full_name_no_formatting_reverse_chinese:0 "$custom_loc_universal_full_name_no_formatting_reverse$"
 custom_loc_universal_full_name_no_formatting_reverse_japanese:0 "[ROOT.GetCharacter.GetLastNameNoFormatting][ROOT.GetCharacter.GetFirstNameNoFormatting]"

 # GetFullNameWithTitleNoFormatting
 custom_loc_universal_full_name_with_title_no_formatting_default:0 "$custom_loc_universal_title$$custom_loc_universal_full_name_no_formatting_default$"
 custom_loc_universal_full_name_with_title_no_formatting_reverse:0 "$custom_loc_universal_title$$custom_loc_universal_full_name_no_formatting_reverse$"
 custom_loc_universal_full_name_with_title_no_formatting_reverse_chinese:0 "$custom_loc_universal_title$$custom_loc_universal_full_name_no_formatting_reverse_chinese$"
 custom_loc_universal_full_name_with_title_no_formatting_reverse_japanese:0 "$custom_loc_universal_title$$custom_loc_universal_full_name_no_formatting_reverse_japanese$"

 # Title
 custom_loc_universal_title:0 "[ROOT.GetCharacter.GetCustom('GetUniversalTitle')]"
 custom_loc_universal_title_default:0 "[SelectLocalization( ROOT.GetCharacter.IsGeneral, 'custom_loc_universal_title_general', SelectLocalization( ROOT.GetCharacter.IsAdmiral, 'custom_loc_universal_title_admiral', '' ) )]"
 custom_loc_universal_title_ruler:0 "[ROOT.GetCharacter.GetCountry.GetRuler.GetPrimaryRoleTitle]$custom_loc_universal_nbsp$"
 custom_loc_universal_title_heir:0 "[ROOT.GetCharacter.GetCountry.GetHeir.GetPrimaryRoleTitle]$custom_loc_universal_nbsp$"
 custom_loc_universal_title_general:0 "$concept_general$$custom_loc_universal_nbsp$"
 custom_loc_universal_title_admiral:0 "$concept_admiral$$custom_loc_universal_nbsp$"

 # Special Characters
 custom_loc_universal_nbsp:0 " "
 custom_loc_universal_dot:0 "・"
 custom_loc_universal_dot_formatted:0 "#tooltippable_name $custom_loc_universal_dot$#!"
