je_didingyin = {
	icon = "gfx/interface/icons/event_icons/event_fire.dds"

	immediate = {
		set_variable = {
			name = import_level_sum
			value = 0
		}
		set_variable = {
			name = export_level_sum
			value = 0
		}
		set_variable = {
			name = level_sum
			value = 0
		}
		set_variable = {
			name = export_level_sum_per_level_sum
			value = 0
		}
		market = {
			every_trade_route = {
				if = {
					limit = { exporter = PREV } # 수출에 해당하는 무역로
					ROOT = {
						change_variable = {
							name = export_level_sum
							add = {
								value = PREV.level
								multiply = { # 규모의 경제 계산
									value = PREV.level
									max = NDefines.NEconomy.TRADE_ROUTE_ECONOMY_OF_SCALE_MAX_LEVEL # 이거 되는 거 맞아??? TODO
									subtract = 1
									multiply = NDefines.NEconomy.TRADE_ROUTE_ECONOMY_OF_SCALE
									add = 1
								}
							}
						}
					}
				}
				else = { # 수입에 해당하는 무역로
					ROOT = {
						change_variable = {
							name = import_level_sum
							add = {
								value = PREV.level
								multiply = { # 규모의 경제 계산
									value = PREV.level
									max = NDefines.NEconomy.TRADE_ROUTE_ECONOMY_OF_SCALE_MAX_LEVEL # 이거 되는 거 맞아??? TODO
									subtract = 1
									multiply = NDefines.NEconomy.TRADE_ROUTE_ECONOMY_OF_SCALE
									add = 1
								}
							}
						}
					}
				}
			}
		}
		change_variable = {
			name = level_sum
			add = var:import_level_sum
		}
		change_variable = {
			name = level_sum
			add = var:export_level_sum
		}
		change_variable = {
			name = export_level_sum_per_level_sum
			add = var:export_level_sum
		}
		change_variable = {
			name = export_level_sum_per_level_sum
			divide = var:level_sum
		}

		if = {
			limit = { has_modifier = didingyin0 }
			remove_modifier = didingyin0
		}
		if = {
			limit = { has_modifier = didingyin1 }
			remove_modifier = didingyin1
		}

		# (export_level_sum_per_level_sum = 0.5) 면 0.3배
		# (export_level_sum_per_level_sum = 0.6667) 면 효과 없음

		if = {
			limit = { var:export_level_sum_per_level_sum < 0.6667 }
			add_modifier = {
				name = didingyin1
				multiplier = {
					value = 0.6667
					subtract = var:export_level_sum_per_level_sum
					multiply = 9
					divide = 5
				}
			}
		}
		else = {
			add_modifier = {
				name = didingyin0
				multiplier = {
					value = var:export_level_sum_per_level_sum
					subtract = 0.6667
					multiply = 9
					divide = 5
				}
			}
		}
	}

	fail = {
		NOT = { has_law = law_type:law_land_based_taxation }
	}

	on_monthly_pulse = {
		effect = { # 지정은제 효과 변동
			set_variable = {
				name = import_level_sum
				value = 0
			}
			set_variable = {
				name = export_level_sum
				value = 0
			}
			set_variable = {
				name = level_sum
				value = 0
			}
			set_variable = {
				name = export_level_sum_per_level_sum
				value = 0
			}
			market = {
				every_trade_route = {
					if = {
						limit = { exporter = PREV } # 수출에 해당하는 무역로
						ROOT = {
							change_variable = {
								name = export_level_sum
								add = {
									value = PREV.level
									multiply = { # 규모의 경제 계산
										value = PREV.level
										max = NDefines.NEconomy.TRADE_ROUTE_ECONOMY_OF_SCALE_MAX_LEVEL # 이거 되는 거 맞아??? TODO
										subtract = 1
										multiply = NDefines.NEconomy.TRADE_ROUTE_ECONOMY_OF_SCALE
										add = 1
									}
								}
							}
						}
					}
					else = { # 수입에 해당하는 무역로
						ROOT = {
							change_variable = {
								name = import_level_sum
								add = {
									value = PREV.level
									multiply = { # 규모의 경제 계산
										value = PREV.level
										max = NDefines.NEconomy.TRADE_ROUTE_ECONOMY_OF_SCALE_MAX_LEVEL # 이거 되는 거 맞아??? TODO
										subtract = 1
										multiply = NDefines.NEconomy.TRADE_ROUTE_ECONOMY_OF_SCALE
										add = 1
									}
								}
							}
						}
					}
				}
			}
			change_variable = {
				name = level_sum
				add = var:import_level_sum
			}
			change_variable = {
				name = level_sum
				add = var:export_level_sum
			}
			change_variable = {
				name = export_level_sum_per_level_sum
				add = var:export_level_sum
			}
			change_variable = {
				name = export_level_sum_per_level_sum
				divide = var:level_sum
			}

			if = {
				limit = { has_modifier = didingyin0 }
				remove_modifier = didingyin0
			}
			if = {
				limit = { has_modifier = didingyin1 }
				remove_modifier = didingyin1
			}

			# (export_level_sum_per_level_sum = 0.5) 면 0.3배
			# (export_level_sum_per_level_sum = 0.7) 면 효과 없음

			if = {
				limit = { var:export_level_sum_per_level_sum < 0.7 }
				add_modifier = {
					name = didingyin1
					multiplier = {
						value = 0.7
						subtract = var:export_level_sum_per_level_sum
						multiply = 9
						divide = 5
					}
				}
			}
			else = {
				add_modifier = {
					name = didingyin0
					multiplier = {
						value = var:export_level_sum_per_level_sum
						subtract = 0.7
						multiply = 9
						divide = 5
					}
				}
			}
		}
	}

	weight = 100
}
