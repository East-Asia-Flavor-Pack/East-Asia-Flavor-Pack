default_popup = {
	name = "hokkaido_open"
	visible = "[GetVariableSystem.Exists('hokkaido_open')]"
	blockoverride "window_header_name" {
	text = "hokkaido_daimyo_info"
	}

	blockoverride "entire_scrollarea" 
	{
		widget = {
			size = { 600 300 }
			button = {
				texture = "gfx/interface/japan_gui/han/etc_base.png"
				tooltip = "han_etc"
				scale = 0.2
				parentanchor = hcenter
			}
			button = {
				texture = "gfx/interface/japan_gui/han/matsumae_base.png"
				tooltip = "han_matsumae"
				scale = 0.2
				parentanchor = hcenter
				position = { -187 208 }
				onclick = "[GetVariableSystem.Toggle('matsumae_open')]"
				modify_texture = {
					visible = "[GreaterThanOrEqualTo_CFixedPoint( GetPlayer.MakeScope.ScriptValue('matsumae_chusei_value'), '(CFixedPoint)80' )]"
					texture = "gfx/interface/japan_gui/extreamly_disloyalty.png"
					blend_mode = normal
				}
				modify_texture = {
					visible = "[And(LessThan_CFixedPoint( GetPlayer.MakeScope.ScriptValue('matsumae_chusei_value'), '(CFixedPoint)80' ), GreaterThanOrEqualTo_CFixedPoint( GetPlayer.MakeScope.ScriptValue('matsumae_chusei_value'), '(CFixedPoint)60' ))]"
					texture = "gfx/interface/japan_gui/high_disloyalty.png"
					blend_mode = normal
				}
				modify_texture = {
					visible = "[And(LessThan_CFixedPoint( GetPlayer.MakeScope.ScriptValue('matsumae_chusei_value'), '(CFixedPoint)60' ), GreaterThanOrEqualTo_CFixedPoint( GetPlayer.MakeScope.ScriptValue('matsumae_chusei_value'), '(CFixedPoint)40' ))]"
					texture = "gfx/interface/japan_gui/disloyalty.png"
					blend_mode = normal
				}
				modify_texture = {
					visible = "[And(LessThan_CFixedPoint( GetPlayer.MakeScope.ScriptValue('matsumae_chusei_value'), '(CFixedPoint)40' ), GreaterThanOrEqualTo_CFixedPoint( GetPlayer.MakeScope.ScriptValue('matsumae_chusei_value'), '(CFixedPoint)20' ))]"
					texture = "gfx/interface/japan_gui/neutral.png"
					blend_mode = normal
				}
				modify_texture = {
					visible = "[And(LessThan_CFixedPoint( GetPlayer.MakeScope.ScriptValue('matsumae_chusei_value'), '(CFixedPoint)10' ), GreaterThanOrEqualTo_CFixedPoint( GetPlayer.MakeScope.ScriptValue('matsumae_chusei_value'), '(CFixedPoint)20' ))]"
					texture = "gfx/interface/japan_gui/low_loyalty.png"
					blend_mode = normal
				}
				modify_texture = {
					visible = "[LessThanOrEqualTo_CFixedPoint( GetPlayer.MakeScope.ScriptValue('matsumae_chusei_value'), '(CFixedPoint)10' )]"
					texture = "gfx/interface/japan_gui/loyalty.png"
					blend_mode = normal
				}
			}
		}
	}
	blockoverride "fixed_bottom" {
		flowcontainer = {
			parentanchor = hcenter
			direction = horizontal
			margin_top = 20
			spacing = 20

			button = {
				using = default_button
				using = close_button_sound
				size = { 250 42 }
				text = "Close"
				onclick = "[GetVariableSystem.Toggle('hokkaido_open')]"
			}
		}
	}
}

default_popup = {
	name = "matsumae_open"
	visible = "[GetVariableSystem.Exists('matsumae_open')]"
	blockoverride "window_header_name" {
	text = "matsumae_daimyo_info"
	}

	blockoverride "entire_scrollarea" 
	{
		widget = {
			background = { #배경
				texture = "gfx/interface/japan_gui/nakagawa.png"
				alpha = 0.1
				fittype = center
			}
			size = { 400 400 }
			datamodel = "[GetGlobalList('matsumae_list')]"
			default_header_clean_faded = {
						size = { 160 40 }
						parentanchor = hcenter
						blockoverride "text" {
							text = "hanshu"
						}
			}
			item = {
				vbox = {
					datacontext = "[Scope.GetCharacter]"
					size = { 400 250 }
					character_portrait_medium_torso = {
						using = tooltip_below
						blockoverride "show_hat" {}
						blockoverride "highlight" {}
					}
					default_header_clean_faded = {
						size = { 160 40 }
						minimumsize = { 120 -1 }
						blockoverride "text" {
							text = "[Character.GetFullName]"
						}
					}
					textbox = {
						autoresize = yes
						align = hcenter|nobaseline
						margin_bottom = 10
						text = "matsumae_info"
					}
				}

			}
		}
	}

	blockoverride "fixed_bottom" {
		flowcontainer = {
			parentanchor = hcenter
			direction = horizontal
			margin_top = 20
			spacing = 20

			button = {
				using = default_button
				using = close_button_sound
				size = { 250 42 }
				text = "Close"
				onclick = "[GetVariableSystem.Toggle('matsumae_open')]"
			}
		}
	}
}

